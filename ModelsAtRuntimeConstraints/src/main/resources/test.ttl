schema:ProjectShape
    a dm:4282980a-bf20-4f75-8f7a-434f6f8e7a54 ;
    sh:targetClass dm:Project;
    sh:property [
          sh:path attribute:Project-854c2eaf-5201-458a-aaf8-273ba23e3cc5-Deadline ;
          sh:node dm:hello ;
          sh:node dm:Project-854c2eaf-5201-458a-aaf8-273ba23e3cc5-Deadline-value ;
      ] ;.


dm:hello a sh:NodeShape ;
         sh:property [
               sh:path attribute:value ;
               sh:datatype xsd:int ;
           ] ;
.

dm:Project-854c2eaf-5201-458a-aaf8-273ba23e3cc5-Deadline-value a sh:NodeShape ;
                                                               sh:property [
                                                                     sh:path attribute:value ;
                                                                     sh:in ( "1" "2" ) ;
                                                                 ] ;
.
